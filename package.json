  "gravityflow": {
    "paths": {
      "root": "",
      "css_dist": "css/",
      "js_dist": "js/",
      "dev": "dev/",
      "fonts": "fonts/",
      "legacy_css": "legacy/css/",
      "css_src": "src/css/",
      "js_src": "src/js/",
      "images": "images/",
      "postcss_assets_base_url": "../",
      "npm": "node_modules"
    }
  },
  "browserslist": {
    "admin": [
      ">0.25%",
      "not ie 11",
      "not op_mini all"
    ],
    "theme": [
      ">0.25%",
      "not op_mini all"
    ]
  },
  "engines": {
    "node": "14.15.0",
    "npm": "6.14.8"
  },
  "scripts": {
    "gulp": "gulp",
    "start": "npm install && npm run gulp -- dev",
    "dev": "npm run gulp -- dev",
    "dist": "npm run gulp -- dist",
    "validate": "npm run gulp -- validate",
    "js:build": "npx webpack",
    "js:theme:dev": "cross-env NODE_ENV=themedev BROWSERSLIST_ENV=theme npx webpack",
    "js:admin:dev": "cross-env NODE_ENV=admindev BROWSERSLIST_ENV=admin npx webpack",
    "js:theme:prod": "cross-env NODE_ENV=themeprod BROWSERSLIST_ENV=theme npx webpack",
    "js:admin:prod": "cross-env NODE_ENV=adminprod BROWSERSLIST_ENV=admin npx webpack",
    "app:example": "cross-env NODE_ENV=example webpack serve --https",
    "bltheme": "cross-env NODE_ENV=themedev BROWSERSLIST_ENV=theme npx browserslist",
    "bladmin": "cross-env NODE_ENV=admindev BROWSERSLIST_ENV=admin npx browserslist",
    "test": "cross-env NODE_ENV=test jest --config jest.config.json"
  },
